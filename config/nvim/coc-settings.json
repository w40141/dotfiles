{
  "diagnostic.errorSign": "\uF490",
  "diagnostic.warningSign": "\uF4A3",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.snippetIndicator": " â–º",
  "suggest.enablePreview": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.noselect": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "json",
    "javascriptreact"
  ],
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.local/bin/black",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "~/.local/bin/flake8",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "~/.local/bin/mypy",
  "python.linting.pydocstyleEnabled": true,
  "python.analysis.useLibraryCodeForTypes": false,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "/Users/daisuke_oku/.asdf/shims/java"
    }
  ],
  "tsserver.tsdk": ".yarn/sdks/typescript/lib",
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "codeLens.enable": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      // custom config path
      // "args": ["-c", "/path/to/your/config.yaml"],
      "filetypes": ["vim", "eruby", "markdown", "yaml"]
    }
  },
  "git.enableGutters": false,
  "git.gitlab.hosts": [
    "https://www.togawa.cs.waseda.ac.jp/gitlab",
    "gitlab.com"
  ]
}
