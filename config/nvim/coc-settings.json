{
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.floatConfig": {
    "border": true,
    "highlight": "CocSuggestFloating"
  },
  "snippets.expands": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "suggest.triggerAfterInsertEnter": true,
  "suggest.minTriggerInputLength": 2,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "suggest.snippetIndicator": " ►",
  "suggest.enablePreview": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.noselect": true,
  "suggest.floatConfig": {
    "border": true,
    "highlight": "CocSuggestFloating"
  },
  "hover.floatConfig": {
    "border": true,
    "highlight": "CocSuggestFloating"
  },
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "json"],
  "coc.preferences.formatOnType": true,
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "~/.local/bin/black",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "~/.local/bin/pflake8",
  "python.linting.mypyEnabled": true,
  "python.linting.mypyPath": "~/.local/bin/mypy",
  "python.linting.pydocstyleEnabled": true,
  "python.analysis.useLibraryCodeForTypes": false,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-11",
      "path": "~/.asdf/installs/java/corretto-11.0.13.8.1",
      "default": true
    }
  ],
  "java.format.enable": true,
  "java.format.settings.profile": "google-java-format",
  "java.saveActions.organizeImports": false,
  "tsserver.tsdk": ".yarn/sdks/typescript/lib",
  "tsserver.enableJavascript": false,
  "workspace.workspaceFolderCheckCwd": false,
  "elixirLS.mixEnv": "dev",
  "elixirLS.mixTarget": "test",
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "eslint.autoFixOnSave": true,
  "eslint.autoFix": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "react"
  ],
  "codeLens.enable": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "languageserver": {
    "clangd": {
      "command": "clangd",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": ["c", "cpp", "objc", "objcpp"]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["vim", "eruby", "markdown", "yaml"]
    },
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact"],
      "initializationOptions": {},
      "requireRootPattern": true,
      "settings": {},
      "rootPatterns": [".flowconfig"]
    }
  },
  "git.enableGutters": false,
  "git.gitlab.hosts": [
    "https://www.togawa.cs.waseda.ac.jp/gitlab",
    "gitlab.com"
  ],
  "cSpell.userWords": [
    "Cinii",
    "Datetime",
    "KAKEN",
    "Prost",
    "Researchmap",
    "Untrusted",
    "Varchar",
    "actix",
    "automock",
    "chrono",
    "codegen",
    "consts",
    "dotenv",
    "eval",
    "getset",
    "gprc",
    "grpc",
    "hashset",
    "issn",
    "jsps",
    "miscs",
    "mockall",
    "nanos",
    "protobuf",
    "thiserror",
    "xorshift"
  ]
}
